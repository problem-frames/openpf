namespace requirements;
import nemf.packages.ontology;

machine evolutionRules {

	pattern provides(Act, Res) = {
		Actor(Act);
		Provides(Pro);
		Resource(Res);
	}
	
	@Trigger('autoStart'='true')
	gtrule provides_pattern(inout Act, inout Res) =  {
		precondition find provides(Act, Res)
		action {
				println("evolution rule caught the pattern provides: " + Act + " -> " + Res);
		}
	} 				
	
}