            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="uk.ac.open.problem.diagram.navigator.ProblemDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="problem.diagram.navigatorActionProvider"
            class="uk.ac.open.problem.diagram.navigator.ProblemNavigatorActionProvider">
         <enablement>
            <or>
