         <enablement>
            <test
               property="org.eclipse.gmf.runtime.emf.core.editingDomain"
               value="dependency.diagram.EditingDomain"/>
         </enablement>
      </clientContext> 
      <binding context="dependency.diagram.TypeContext">
         <elementType ref="dependency.diagram.Model_1000"/>
         <elementType ref="dependency.diagram.Node_2001"/>
         <elementType ref="dependency.diagram.Coupling_4001"/>
         <elementType ref="dependency.diagram.Precedence_4002"/>
         <advice ref="org.eclipse.gmf.runtime.diagram.core.advice.notationDepdendents"/>
      </binding>
   </extension>
