   <extension point="org.eclipse.ui.navigator.navigatorContent" id="navigator-content">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="dependency.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="uk.ac.open.dependency.dependency.diagram.navigator.DependencyNavigatorContentProvider" 
            labelProvider="uk.ac.open.dependency.dependency.diagram.navigator.DependencyNavigatorLabelProvider"
            icon="icons/obj16/DependencyDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="dependency_diagram"/>
               </and>
               <instanceof value="uk.ac.open.dependency.dependency.diagram.navigator.DependencyAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="uk.ac.open.dependency.dependency.diagram.navigator.DependencyAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="dependency.diagram.navigatorSorter" 
               class="uk.ac.open.dependency.dependency.diagram.navigator.DependencyNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="dependency_diagram"/>
                  </and>
                  <instanceof value="uk.ac.open.dependency.dependency.diagram.navigator.DependencyAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="dependency.diagram.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="uk.ac.open.dependency.dependency.diagram.navigator.DependencyDomainNavigatorContentProvider" 
            labelProvider="uk.ac.open.dependency.dependency.diagram.navigator.DependencyDomainNavigatorLabelProvider"
            icon="icons/obj16/DependencyDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="dependency"/>
               </and>
               <instanceof value="uk.ac.open.dependency.dependency.diagram.navigator.DependencyDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="uk.ac.open.dependency.dependency.diagram.navigator.DependencyDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="dependency.diagram.navigatorActionProvider"
            class="uk.ac.open.dependency.dependency.diagram.navigator.DependencyNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="uk.ac.open.dependency.dependency.diagram.navigator.DependencyAbstractNavigatorItem"/>
            </or>
         </enablement>
      </actionProvider>
   </extension>
   
	   <extension point="org.eclipse.ui.navigator.linkHelper" id="navigator-linkhelper">
      <?gmfgen generated="true"?>
      <linkHelper
            id="dependency.diagram.navigatorLinkHelper"
            class="uk.ac.open.dependency.dependency.diagram.navigator.DependencyNavigatorLinkHelper">
         <editorInputEnablement>
            <and>
               <instanceof value="org.eclipse.emf.common.ui.URIEditorInput"/>
               <test property="dependency.diagram.isURIEditorInput"/>
            </and>
         </editorInputEnablement>
         <selectionEnablement>
            <instanceof value="uk.ac.open.dependency.dependency.diagram.navigator.DependencyAbstractNavigatorItem"/>
         </selectionEnablement>
      </linkHelper>
   </extension>
