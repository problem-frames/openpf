<?xml version="1.0" encoding="ASCII"?>
<gmfmap:Mapping xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="Argument.ecore#//ArgumentDiagram/nodes"/>
    <ownedChild
        relatedDiagrams="//@diagram">
      <domainMetaElement
          href="Argument.ecore#//Argument"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping"
          viewPattern="{0}: {1}\n {2} #{3}"
          editorPattern="{0}: {1}\n {2} #{3}"
          editPattern="{0}: {1}\n {2} #{3}">
        <diagramLabel
            href="Argument.gmfgraph#ArgumentLabel"/>
        <features
            href="Argument.ecore#//Argument/name"/>
        <features
            href="Argument.ecore#//Argument/description"/>
        <features
            href="Argument.ecore#//Argument/round"/>
        <features
            href="Argument.ecore#//Argument/expr"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="Argument.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="Argument.gmfgraph#Argument"/>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0"
          referencedChild="//@nodes.0/@ownedChild">
        <containmentFeature
            href="Argument.ecore#//Argument/grounds"/>
      </children>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.1"
          referencedChild="//@nodes.0/@ownedChild">
        <containmentFeature
            href="Argument.ecore#//Argument/warrants"/>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.0">
        <compartment
            href="Argument.gmfgraph#ArgumentGroundsCompartment"/>
      </compartments>
      <compartments
          children="//@nodes.0/@ownedChild/@children.1">
        <compartment
            href="Argument.gmfgraph#ArgumentWarrantsCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="Argument.ecore#//Rebuts"/>
    <containmentFeature
        href="Argument.ecore#//ArgumentDiagram/links"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="Argument.gmftool#//@palette/@tools.1/@tools.2"/>
    <diagramLink
        href="Argument.gmfgraph#Rebuts"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="Argument.ecore#//Link/from"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="Argument.ecore#//Link/to"/>
  </links>
  <links>
    <domainMetaElement
        href="Argument.ecore#//Mitigates"/>
    <containmentFeature
        href="Argument.ecore#//ArgumentDiagram/links"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="Argument.gmftool#//@palette/@tools.1/@tools.0"/>
    <diagramLink
        href="Argument.gmfgraph#Mitigates"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="Argument.ecore#//Link/from"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="Argument.ecore#//Link/to"/>
  </links>
  <links>
    <labelMappings
        xsi:type="gmfmap:DesignLabelMapping"
        readOnly="true">
      <diagramLabel
          href="Argument.gmfgraph#ArgumentOriginExternalLabel"/>
    </labelMappings>
    <tool
        xsi:type="gmftool:CreationTool"
        href="Argument.gmftool#//@palette/@tools.1/@tools.1"/>
    <diagramLink
        href="Argument.gmfgraph#ArgumentOrigin"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="Argument.ecore#//Argument/origin"/>
  </links>
  <diagram>
    <diagramCanvas
        href="Argument.gmfgraph#argument"/>
    <domainModel
        href="Argument.ecore#/"/>
    <domainMetaElement
        href="Argument.ecore#//ArgumentDiagram"/>
    <palette
        href="Argument.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
