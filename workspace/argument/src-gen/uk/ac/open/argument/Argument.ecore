<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="argument"
    nsURI="http://www.ac.uk/open/argument/Argument" nsPrefix="argument">
  <eClassifiers xsi:type="ecore:EClass" name="ArgumentDiagram">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="highlight" eType="#//Node"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodes" upperBound="-1"
        eType="#//Node" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="links" upperBound="-1"
        eType="#//Link" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Node">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//NodeType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="NodeType">
    <eLiterals name="CLAIM" literal="C"/>
    <eLiterals name="WARRANT" value="1" literal="W"/>
    <eLiterals name="GROUND" value="2" literal="G"/>
    <eLiterals name="REBUTTAL" value="3" literal="R"/>
    <eLiterals name="MITIGATION" value="4" literal="M"/>
    <eLiterals name="ARGUMENT" value="5" literal="A"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Link">
    <eStructuralFeatures xsi:type="ecore:EReference" name="afrom" eType="#//Node"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="link" eType="#//LinkType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="toNode1" eType="#//Expression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Expression"/>
  <eClassifiers xsi:type="ecore:EEnum" name="ConnectiveType">
    <eLiterals name="AND" literal="and"/>
    <eLiterals name="OR" value="1" literal="or"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NodeLiteral" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="value" eType="#//Node"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="LinkType">
    <eLiterals name="ARGUES" literal="argues"/>
    <eLiterals name="REBUTS" value="1" literal="rebutted by"/>
    <eLiterals name="MITIGATES" value="2" literal="mitigated by"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SplitCommas" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" eType="#//Expression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" eType="#//Expression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SplitConnective" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" eType="#//Expression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="connective" eType="#//ConnectiveType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" eType="#//Expression"
        containment="true"/>
  </eClassifiers>
</ecore:EPackage>
