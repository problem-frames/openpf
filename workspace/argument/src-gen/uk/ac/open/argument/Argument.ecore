<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="argument"
    nsURI="http://www.ac.uk/open/argument/Argument" nsPrefix="argument">
  <eClassifiers xsi:type="ecore:EClass" name="Arg">
    <eStructuralFeatures xsi:type="ecore:EReference" name="groundstatements" upperBound="-1"
        eType="#//gStatement" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="inner" eType="#//RecArg"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="gStatement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="groundstr" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RecArg">
    <eStructuralFeatures xsi:type="ecore:EReference" name="groundstatements" upperBound="-1"
        eType="#//gStatement" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
