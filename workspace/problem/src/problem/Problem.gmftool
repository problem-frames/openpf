            xsi:type="gmftool:BundleImage"
            path="icons/obj16/rqr_obj.gif"
            bundle="problem"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Constraint"
          description="Create new Requirement Constraint">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="icons/obj16/rqc_obj.gif"
