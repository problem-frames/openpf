          description="Create new Lexical Domain">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="icons/obj16/ldm_obj.gif"
            bundle="problem"/>
        <largeIcon
            xsi:type="gmftool:BundleImage"
            path="icons/obj16/ldm_obj.gif"
            bundle="problem"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Requirement"
