        <largeIcon
            xsi:type="gmftool:BundleImage"
            path="icons/obj16/mch_obj.gif"
            bundle="problem"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Domain"
          description="Create new Domain">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="icons/obj16/dom_obj.gif"
            bundle="problem"/>
        <largeIcon
            xsi:type="gmftool:BundleImage"
            path="icons/obj16/dom_obj.gif"
            bundle="problem"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Biddable Domain"
          description="Create new Biddable Domain">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="icons/obj16/bdm_obj.gif"
